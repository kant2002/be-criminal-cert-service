{
    "name": "criminal-cert-service",
    "version": "1.7.1",
    "description": "Criminal Record Certificate service",
    "author": "Diia",
    "license": "SEE LICENSE IN LICENSE.md",
    "repository": "https://github.com/diia-open-source/be-criminal-cert-service.git",
    "main": "dist/index.js",
    "scripts": {
        "prepare": "ts-patch install -s",
        "build": "rimraf dist/ && mkdir dist && npm run genproto && tsc",
        "start": "node dist/index.js",
        "semantic-release": "semantic-release",
        "lint": "eslint --ext .ts . && prettier --check .",
        "lint-fix": "eslint --ext .ts --fix && prettier --write .",
        "lint:lockfile": "lockfile-lint --path package-lock.json --allowed-hosts registry.npmjs.org --validate-https",
        "test": "jest",
        "test:integration": "npm run test --selectProjects integration --",
        "test:unit": "npm run test --selectProjects unit --",
        "migrate-deploy": "npm run migrate up",
        "migrate-ci": "npm run migrate up",
        "migrate-test": "NODE_ENV=test npm run migrate up",
        "migrate": "sh -c 'ts-node --project migrations/tsconfig.json node_modules/.bin/migrate-mongo $0 $1 -f migrate-mongo-config.ts'",
        "indexes:sync": "MONGO_INDEXES_SYNC=true MONGO_INDEXES_EXIT_AFTER_SYNC=true npm run start",
        "find-circulars": "madge --circular --extensions ts ./",
        "scaffold": "scaffold",
        "genproto": "genproto --outputDir src/generated"
    },
    "keywords": [],
    "engines": {
        "node": ">=18"
    },
    "_moduleAliases": {
        "@services": "dist/services",
        "@interfaces": "dist/interfaces",
        "@models": "dist/models",
        "@actions": "dist/actions",
        "@src": "dist",
        "@tests": "tests"
    },
    "jest": {
        "preset": "@kant2002-diia-inhouse/configs/dist/jest"
    },
    "commitlint": {
        "extends": "@kant2002-diia-inhouse/configs/dist/commitlint"
    },
    "eslintConfig": {
        "extends": "@kant2002-diia-inhouse/eslint-config",
        "parserOptions": {
            "project": [
                "./tsconfig.json",
                "./tests/tsconfig.json",
                "./migrations/tsconfig.json"
            ]
        }
    },
    "release": {
        "extends": "@kant2002-diia-inhouse/configs/dist/semantic-release/service"
    },
    "prettier": "@kant2002-diia-inhouse/eslint-config/prettier",
    "madge": {
        "tsConfig": "./tsconfig.json"
    },
    "dependencies": {
        "@kant2002-diia-inhouse/address-service-client": "^1.0.0-118331",
        "@kant2002-diia-inhouse/analytics": "^1.54.0",
        "@kant2002-diia-inhouse/analytics-service-client": "^1.0.0-121540",
        "@kant2002-diia-inhouse/crypto": "^1.7.0",
        "@kant2002-diia-inhouse/db": "^3.1.1",
        "@kant2002-diia-inhouse/diia-app": "^12.2.0",
        "@kant2002-diia-inhouse/diia-crypto-client": "^1.0.0-94617",
        "@kant2002-diia-inhouse/diia-logger": "^2.10.0",
        "@kant2002-diia-inhouse/diia-metrics": "^3.6.0",
        "@kant2002-diia-inhouse/diia-queue": "^5.10.1",
        "@kant2002-diia-inhouse/documents-service-client": "^1.0.0-121521",
        "@kant2002-diia-inhouse/env": "^1.5.0",
        "@kant2002-diia-inhouse/errors": "^1.6.1",
        "@kant2002-diia-inhouse/healthcheck": "^1.6.0",
        "@kant2002-diia-inhouse/http": "^1.5.3",
        "@kant2002-diia-inhouse/notification-service-client": "^1.0.0-121722",
        "@kant2002-diia-inhouse/public-service-catalog-client": "^1.0.0-115538",
        "@kant2002-diia-inhouse/redis": "^2.4.0",
        "@kant2002-diia-inhouse/types": "^5.1.0",
        "@kant2002-diia-inhouse/user-service-client": "^1.0.0-121341",
        "@kant2002-diia-inhouse/utils": "^2.36.0",
        "@kant2002-diia-inhouse/validators": "^1.13.1",
        "adm-zip": "0.5.10",
        "lodash": "4.17.21",
        "migrate-mongo": "11.0.0",
        "module-alias": "2.2.3",
        "moment": "2.29.4"
    },
    "devDependencies": {
        "@kant2002-diia-inhouse/configs": "^1.27.1",
        "@kant2002-diia-inhouse/eslint-config": "^3.5.0",
        "@kant2002-diia-inhouse/genproto": "^1.10.0",
        "@kant2002-diia-inhouse/scaffold": "^2.8.0",
        "@kant2002-diia-inhouse/test": "^5.1.2",
        "@types/adm-zip": "0.5.1",
        "@types/jest": "29.5.5",
        "@types/lodash": "4.14.202",
        "eslint": "8.53.0",
        "jest": "29.5.0",
        "lockfile-lint": "4.12.1",
        "madge": "6.1.0",
        "prettier": "3.0.3",
        "rimraf": "5.0.5",
        "semantic-release": "21.1.2",
        "ts-node": "10.9.2",
        "ts-patch": "2.1.0",
        "type-fest": "4.8.3",
        "typescript": "5.3.3",
        "typescript-transform-paths": "3.4.6"
    },
    "overrides": {
        "mongoose": "8.2.0",
        "protobufjs": "7.2.6"
    }
}
